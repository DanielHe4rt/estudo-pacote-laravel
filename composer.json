{
    "name": "danielhe4rt/frozentest",
    "description": "Test Package ",
    "homepage": "https://github.com/lab123/frozentest",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Reis",
            "email": "idanielreiss@gmail.com",
            "homepage": "https://danielgreis.me/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "^5.1|^5.2|^5.3"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "frozentest\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "frozentest\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
